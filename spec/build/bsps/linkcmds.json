  "do-build": "bld.install_as(\n    \"${BSP_LIBDIR}/linkcmds\",\n    \"bsps/\" + bld.env.ARCH + \"/\" + bld.env.BSP_FAMILY +\n    \"/start/linkcmds.\" + bld.env.BSP_BASE\n)\n",
